{% extends 'base.html.twig' %}

{% block title %}Sante{% endblock %}

{% block body %}
    <h1>Sante</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sante.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ sante.date ? sante.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ sante.libelle }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ sante.quantite }}</td>
            </tr>
            <tr>
                <th>Heure</th>
                <td>{{ sante.heure ? sante.heure|date('H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sante_index') }}">back to list</a>

    <a href="{{ path('sante_edit', {'id': sante.id}) }}">edit</a>

    {{ include('sante/_delete_form.html.twig') }}
{% endblock %}
